#, c-format
msgid "iTunes directory not found: '%s' (or similar)."
msgstr ""

#: ../src/itdb_itunesdb.c:2592
#, c-format
msgid "Music directory not found: '%s' (or similar)."
msgstr ""

#: ../src/itdb_itunesdb.c:2610
#, c-format
msgid "Control directory not found: '%s' (or similar)."
msgstr ""

