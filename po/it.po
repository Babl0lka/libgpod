#, c-format
msgid "Error renaming '%s' to '%s' (%s)."
msgstr "Errore nella rinominazione da '%s' a '%s' (%s)."

#: ../src/itdb_itunesdb.c:3282
#, c-format
msgid "Error removing '%s' (%s)."
msgstr "Errore nella rimozione di '%s' (%s)."

